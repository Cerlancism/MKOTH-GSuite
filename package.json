{
  "name": "mkoth-gsuite",
  "version": "0.0.2",
  "description": "Spreadsheet and API end points for Mobile KOTH",
  "main": "app/main.js",
  "dependencies": {
    "@types/google-apps-script": "^1.0.9"
  },
  "devDependencies": {
    "typescript": "^3.7.4"
  },
  "scripts": {
    "buildRouter": "tsc --project ./src/app-script-router",
    "build": "node timestamper ./app/code.js & parcel build ./src/web-app/app.ts --out-dir ./app --global MKOTH --no-source-maps --no-minify",
    "push": "npm run build & cd ./app & clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MobileKOTH/MKOTH-GSuite.git"
  },
